<meta:Type
  xmlns:meta="class:/spiralcraft/data/types/meta/"
  xmlns:bizm="class:/spiralcraft/biz/meta/"
  xmlns:entity="class:/spiralcraft/archetype/entity/"
  xmlns:social="class:/spiralcraft/archetype/social/"
  >
  
  <!-- 
    An account associated with receivables and payables
  -->
  <baseType><bizm:Account.type/>
  </baseType>
  
  <fields>
  
    <meta:Field>
      <name>contactId
      </name>
      <type><LSID.type/>
      </type>
    </meta:Field>
    
    <meta:RelativeField>
      <name>contact
      </name>
      
      <type><social:Contact.type/>
      </type>
      
      <fieldList>contactId
      </fieldList>
    </meta:RelativeField>
        
    <meta:Field>
      <name>repId
      </name>
      <type><LSID.type/>
      </type>
    </meta:Field>
    
    <meta:RelativeField>
      <name>rep
      </name>
      
      <type><social:Contact.type/>
      </type>
      
      <fieldList>repId
      </fieldList>
    </meta:RelativeField>


    <meta:Field>
      <name>entityId
      </name>
      <type><LSID.type/>
      </type>
    </meta:Field>
        
    <meta:RelativeField>
      <name>entity
      </name>
      
      <type><entity:Actor.type/>
      </type>
      
      <fieldList>entityId
      </fieldList>
    </meta:RelativeField>



  </fields>
 
</meta:Type>
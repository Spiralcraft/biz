<scm:Change.list
  xmlns:scm="class:/spiralcraft/scm/meta/"
  >
  <scm:Change>
  
    <revision>0.4.17
    </revision>
    
    <authorId>miketoth
    </authorId>
    
    <date>
    </date>

    <detail>
      <scm:Change>
        
        <revision>-dev6
        </revision>
        
        <notes>
          <scm:Note>Add dash-a UI stack for Customer, Project,
            Runs, and Tracker related entities.
          </scm:Note>
          
          <path>package/lib-js/spiralcraft/biz/**
          </path>
        </notes>
      </scm:Change>
      
      <scm:Change>
        
        <revision>-dev6
        </revision>
        
        <notes>
          <scm:Note>Add Customer and Project entities along with associated js
            client views and API.
          </scm:Note>
        </notes>
        
        <resources>
          <URI>source/spiralcraft/biz/meta/Customer.type.xml
          </URI>
          <URI>source/sprialcraft/biz/pm/Project.type.xml
          </URI>
          <URI>package/lib-js/spiralcraft/biz/customerView.js
          </URI>
          <URI>package/lib-js/spiralcraft/biz/projectView.js
          </URI>
          <URI>package/war-webui/api/Customer.assy.xml
          </URI>
          <URI>package/war-webui/api/Project.assy.xml
          </URI>
          <URI>package/war-webui/api/PathContext.assy.xml
          </URI>
        </resources>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev4
        </revision>
        
        <notes>
          <scm:Note>Open new SalesOrder for CustomerAccount
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta
        </path>
        
        <resources>
          <URI>CustomerAccount.type.xml
          </URI>
          <URI>SalesOrder.type.xml
          </URI>
        </resources>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev3
        </revision>
        
        <notes>
          <scm:Note>Add a Receipt type for tracking incoming money.
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta/Receipt.type.xml
        </path>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev3
        </revision>
        
        <notes>
          <scm:Note>Set default precision and scale for Money type
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta/Money.type.xml
        </path>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev3
        </revision>
        
        <notes>
          <scm:Note>Payment terms uses standard spiralcraft Date type 
            instead of java.util.Date class
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta/PaymentTerms.type.xml
        </path>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev3
        </revision>
        
        <notes>
          <scm:Note>SalesOrderDetailSupport provides itemized breakdown to
            support line item.
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta
        </path>
        
        <resources>
          <URI>Biz.schema.xml
          </URI>
          <URI>SalesOrderDetail.type.xml
          </URI>
          <URI>SalesOrderDetailSupport.type.xml
          </URI>
        </resources>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev2
        </revision>
        
        <notes>
          <scm:Note>Make sure that SalesOrder.reorder() returns the new order.
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta/SalesOrder_reorder.scenario.xml
        </path>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev2
        </revision>
        
        <notes>
          <scm:Note>Provide for priorBalance, priorDueDate in SalesInvoice
            .create method.
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta/SalesInvoice_create.scenario.xml
        </path>

      </scm:Change>

      <scm:Change>
        
        <revision>-dev1
        </revision>
        
        <notes>
          <scm:Note>Fix deprecated named struct construct
          </scm:Note>
        </notes>
        
        <path>source/spiralcraft/biz/meta
        </path>
        
        <resources>
          <URI>SalesInvoice_create.scenario.xml
          </URI>
          <URI>SalesOrder_reorder.scenario.xml
          </URI>
        </resources>

      </scm:Change>
      

    </detail>
  </scm:Change>

